package kr.happyjob.jobfolio.vo.Shopping;

public class ShoppingModel {

    private String product_no;
    private int image_type;
    private int image_seq;
    private String product_name;
    private int price;
    private float free_rate;
	private int free_price;
    private String maker;
    private String best;
    private String loginID;
    private String regdate;
    private String file_name;
    private String logical_path;
    private String phygical_path;
    private int file_size;
    private String file_ext;

    public String getProduct_no() {
        return product_no;
    }

    public void setProduct_no(String product_no) {
        this.product_no = product_no;
    }

    public int getImage_type() {
        return image_type;
    }

    public void setImage_type(int image_type) {
        this.image_type = image_type;
    }

    public int getImage_seq() {
        return image_seq;
    }

    public void setImage_seq(int image_seq) {
        this.image_seq = image_seq;
    }

    public String getProduct_name() {
        return product_name;
    }

    public void setProduct_name(String product_name) {
        this.product_name = product_name;
    }

    public int getPrice() {
        return price;
    }

    public void setPrice(int price) {
        this.price = price;
    }
    
    public float getFree_rate() {
		return free_rate;
	}

	public void setFree_rate(float free_rate) {
		this.free_rate = free_rate;
	}
	
    public int getFree_price() {
        return free_price;
    }

    public void setFree_price(int free_price) {
        this.free_price = free_price;
    }

    public String getMaker() {
        return maker;
    }

    public void setMaker(String maker) {
        this.maker = maker;
    }

    public String getBest() {
		return best;
	}

	public void setBest(String best) {
		this.best = best;
	}

    public String getLoginID() {
        return loginID;
    }

    public void setLoginID(String loginID) {
        this.loginID = loginID;
    }

    public String getRegdate() {
        return regdate;
    }

    public void setRegdate(String regdate) {
        this.regdate = regdate;
    }

    public String getFile_name() {
        return file_name;
    }

    public void setFile_name(String file_name) {
        this.file_name = file_name;
    }

    public String getLogical_path() {
        return logical_path;
    }

    public void setLogical_path(String logical_path) {
        this.logical_path = logical_path;
    }

    public String getPhygical_path() {
        return phygical_path;
    }

    public void setPhygical_path(String phygical_path) {
        this.phygical_path = phygical_path;
    }

    public int getFile_size() {
        return file_size;
    }

    public void setFile_size(int file_size) {
        this.file_size = file_size;
    }

    public String getFile_ext() {
        return file_ext;
    }

    public void setFile_ext(String file_ext) {
        this.file_ext = file_ext;
    }




}
